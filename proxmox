sudo apt-get update  && sudo apt-get full-upgrade -y
echo "deb http://deb.debian.org/debian bullseye main contrib non-free" | sudo tee /etc/apt/sources.list
echo "deb http://deb.debian.org/debian-security/ bullseye-security main contrib non-free" | sudo tee /etc/apt/sources.list
echo "deb http://deb.debian.org/debian bullseye-backports main contrib non-free" | sudo tee /etc/apt/sources.list
echo "deb http://deb.debian.org/debian bullseye-updates main contrib non-free" | sudo tee /etc/apt/sources.list
echo "deb http://deb.debian.org/debian unstable main" | sudo tee /etc/apt/sources.list
